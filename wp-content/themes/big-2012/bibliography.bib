@INPROCEEDINGS{5602795,
  author = {Takahashi, M. and Shinoda, H.},
  title = {Large aperture Airborne Ultrasound Tactile Display using distributed
	array units},
  booktitle = {SICE Annual Conference 2010, Proceedings of},
  year = {2010},
  pages = {359 -362},
  month = {aug.},
  abstract = {Airborne Ultrasound Tactile Display (AUTD), which is developed in
	our laboratory, can provide tactile sensation in midair using acoustic
	radiation pressure without any equipment attached on your hands.
	It can create various stimulus patterns in arbitrary point by steering
	the focal point using a linear phased array. However, its generating
	power is only 1.6 #x00D7;10-2 N and the "arbitrary point" is limited
	to a small space, so there is a need for advancement of its performance.
	To avoid these weaknesses of AUTD, we try to realize a large aperture
	of array by synchronized multiple AUTD units. In this paper, we describe
	the detail of the proposed method, the numerical simulation for determine
	the arrangement of AUTD units, and newer prototype now under construction.},
  file = {:/Users/tc7949/Dropbox/phd/papers/library/Takahashi.2010.Large_Aperture_Airbourne_Ultrasound.pdf:PDF},
  keywords = {acoustic radiation pressure;distributed array units;focal point;large
	aperture airborne ultrasound tactile display;linear phased array;numerical
	simulation;stimulus patterns;tactile sensation;haptic interfaces;three-dimensional
	displays;ultrasonic devices;},
  owner = {tc7949},
  review = {2011-11-28},
  timestamp = {2011.12.02},
  url = {http://ieeexplore.ieee.org/xpls/abs_all.jsp?arnumber=5602795&tag=1}
}

