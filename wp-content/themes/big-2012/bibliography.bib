  booktitle = {Haptics: Generating and Perceiving Tangible Sensations},
  publisher = {Springer Berlin / Heidelberg},
  year = {2010},
  editor = {Kappers, Astrid and van Erp, Jan and Bergmann Tiest, Wouter and van
