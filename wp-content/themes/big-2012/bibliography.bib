@ARTICLE{4674348,
  author = {MacLean, K.E.},
  title = {Foundations of Transparency in Tactile Information Design},
  journal = {Haptics, IEEE Transactions on},
  year = {2008},
  volume = {1},
  pages = {84 -95},
  number = {2},
  month = {july-dec. },
  abstract = {This paper places contemporary literature on the topic of unimodal,
	single-site display of information using complex tactile signals
	in the context of progress towards the achievement of transparent
	communication - placing minimal load on the user's attentional resources.
	We discuss recent evidence that more is possible with purely haptic
	display than is commonly believed, as well as procedural developments
	that support systematic design of transparent tactile information
	display; and we frame the advances required to realize significant
	benefits with the technology we have now. Examples used and objectives
	thus identified focus on establishing effective information representations,
	and outlining efficient tools and processes for perceptually guiding
	icon design. Our discussion is inspired by Weiser's vision of calm
	technology based on locatedness and seamless movement between center
	and periphery, and it is organized along the lines of potential utility,
	form and learning.},
  doi = {10.1109/TOH.2008.20},
  file = {:/Users/tc7949/Dropbox/phd/papers/library/MacLean.2008.Foundations_of_Transparency_in_Tactile_Information_Design.pdf:PDF},
  issn = {1939-1412},
  keywords = {calm technology;haptic display;icon design;information display;information
	representation;tactile information design;transparency;transparent
	communication;unimodal single-site display;user attentional resources;graphical
	user interfaces;haptic interfaces;user centred design;},
  owner = {tc7949},
